{
  "name": "compli-guard",
  "version": "1.0.0",
  "description": "Privacy-Preserving Compliance Enforcement with Chainlink CRE",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "server": "ts-node src/api/server.ts",
    "workflow": "ts-node src/cre/run.ts",
    "demo": "ts-node src/demo.ts",
    "demo:local": "node -e \"console.log('Terminal 1: npm run server'); console.log('Terminal 2: npm run workflow');\""
  },
  "keywords": [
    "compliance",
    "chainlink",
    "cre",
    "privacy",
    "stablecoin",
    "reserves",
    "enforcement"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@chainlink/cre-sdk": "^1.0.9",
    "@types/cors": "^2.8.19",
    "cors": "^2.8.6",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "viem": "^2.45.3",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
